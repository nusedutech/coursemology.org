	function popupform(form){
    	if (!window.focus) {
    		return true;
    	}

    	window.open('', 'popup', 'height=610, width=610, scrollbars=yes');
    	form.target = 'popup';

    	return true;
    }

    function haltUp() {
    	loadingCount += 1;
    	halt();
    }

    function resumeDown() {
    	loadingCount -= 1;
    	if (loadingCount == 0) {
    		resume();
    	}
    }
