<% tags.each do |tag| %>
    <% earned = tag.all_raw_correct_answer_attempts(@user_course).size %>
    <% max = earned + tag.all_raw_wrong_answer_attempts(@user_course).size %>
    <% earned =[earned, max].min %>
    <tr>
      <td><%= tag.name %></td>
      <td>
        <div class="progress" style="margin-bottom: 3px">
          <div class="bar bar-success" style="width: <%= max == 0 ? 0 : earned / max.to_f * 100%>%"></div>
        </div>
      </td>
      <td><%= earned %></td>
      <td>/</td>
      <td><%= max %></td>
    </tr>
<% end %>
